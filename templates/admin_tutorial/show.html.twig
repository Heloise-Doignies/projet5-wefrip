{% extends 'base-backend.html.twig' %}

{% block title %}Tutorial{% endblock %}

{% block body %}
    <h1>Tutorial</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tutorial.id }}</td>
            </tr>
            <tr>
                <th>TutoName</th>
                <td>{{ tutorial.tutoName }}</td>
            </tr>
            <tr>
                <th>TutoDescription</th>
                <td>{{ tutorial.tutoDescription }}</td>
            </tr>
            <tr>
                <th>TutoFileName</th>
                <td>{{ tutorial.tutoFileName }}</td>
            </tr>
            <tr>
                <th>TutoVideoName</th>
                <td>{{ tutorial.tutoVideoName }}</td>
            </tr>
            <tr>
                <th>TutoImageName</th>
                <td>{{ tutorial.tutoImageName }}</td>
            </tr>
            <tr>
                <th>TutoSupportType</th>
                <td>{{ tutorial.tutoSupportType }}</td>
            </tr>
            <tr>
                <th>TutoSlug</th>
                <td>{{ tutorial.tutoSlug }}</td>
            </tr>
            <tr>
                <th>TutoUpdatedAt</th>
                <td>{{ tutorial.tutoUpdatedAt ? tutorial.tutoUpdatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_tutorial_index') }}">back to list</a>

    <a href="{{ path('app_admin_tutorial_edit', {'id': tutorial.id}) }}">edit</a>

    {{ include('admin_tutorial/_delete_form.html.twig') }}
{% endblock %}
